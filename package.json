{
  "name": "atom-hexo",
  "main": "./lib/hexo",
  "version": "1.0.0",
  "description": "Hexo for the Atom Editor.",
  "activationCommands": {
    "atom-workspace": [
      "atom-hexo:new",
      "atom-hexo:publish",
      "atom-hexo:generate",
      "atom-hexo:deploy",
      "atom-hexo:clean"
    ]
  },
  "repository": "https://github.com/heroicyang/atom-hexo",
  "author": {
    "name": "Heroic Yang",
    "email": "me@heroicyang.com",
    "url": "http://heroicyang.com/"
  },
  "license": "MIT",
  "engines": {
    "atom": ">=0.174.0 <2.0.0"
  },
  "dependencies": {
    "atom-space-pen-views": "^2.0.5",
    "hexo": "^3.0.1",
    "minimist": "^1.1.1",
    "untildify": "^2.0.0"
  },
  "consumedServices": {
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    }
  },
  "devDependencies": {
    "babel-eslint": "^3.1.4",
    "eslint": "^0.21.1"
  }
}
